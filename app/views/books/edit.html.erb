<%= render :partial => 'shared/book', :locals => { :book => @book, :user => nil } %>
<p>
<%= form_tag(book_path(@book.asin), :remote => :true, :method => :put) do %>
  <%= label_tag 'Start Date' %><%= text_field_tag 'start_date', format_date(@log.start_date) %>
  <%= label_tag 'Finish Date' %><%= text_field_tag 'finish_date', format_date(@log.finish_date) %>
  <%= submit_tag 'Update' %>
<% end %>
</p>
